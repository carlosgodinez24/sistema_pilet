	<queryString>
		<![CDATA[select c.codi_usua as 'codiUsua', (case(select count(*) from visitante v inner join visitante_cita vcs on v.codi_visi = vcs.codi_visi and vcs.codi_cita = $P{codi_cita})when 1 then (select CONCAT(v.nomb_visi, ' ', v.apel_visi) from visitante v inner join visitante_cita vcs on v.codi_visi = vcs.codi_visi and vcs.codi_cita = $P{codi_cita}) else 'Grupo de Visitantes' end) as 'Visitante(s)', CONCAT(cc.fech_camb_cita, ' ', cc.hora_camb_cita ) as 'Fecha y Hora del Cambio', case (cc.esta_camb_cita) when 0 then 'ELIMINADA' when 1 then 'SOLICITADA' when 2 then 'PROGRAMADA' when 3 then 'RECHAZADA' when 4 then 'REPROGRAMACION SOLICITADA' when 5 then 'CANCELACION SOLICITADA' when 6 then 'FINALIZADA ' END AS 'Cambio Realizado', CONCAT(cc.fech_inic_cita_nuev, ' ', cc.hora_inic_cita_nuev)as 'Fecha y Hora de Inicio Nueva', CONCAT(cc.fech_fin_cita_nuev, ' ', cc.hora_fin_cita_nuev)as 'Fecha y Hora de Finalizaci贸n Nueva', (case(cc.moti_camb_cita) when NULL then '-' else cc.moti_camb_cita end) as'Motivo del Cambio', c.desc_cita as'Motivo de la Cita' from cambio_cita cc inner join cita c on cc.codi_cita = c.codi_cita and c.codi_cita = $P{codi_cita}]]>
	</queryString>
